<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" editor-extension-mode="False">
    <Style src="project://database/Assets/TableForge/Editor/UI/Windows/ExportImportWindow/ImportWIndow/ImportWindow.uss?fileID=7433441132597879392&amp;guid=479796cd6a9c48a88c05037cccac3aa2&amp;type=3#ImportWindow" />
    <ui:ScrollView>
        <ui:VisualElement name="data-processing-container" class="step-container">
            <ui:Label text="Import Table Data" class="step-title" />
            <ui:TextField label="Table Name" name="table-name-field" />
            <uie:EnumField label="Data Format" name="format-field" />
            <ui:Toggle label="First row contains headers" name="csv-header-toggle" />
