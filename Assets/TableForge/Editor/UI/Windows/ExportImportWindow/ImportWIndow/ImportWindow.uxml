                <uie:DropdownField label="Namespace" name="namespace-dropdown" />
                <uie:DropdownField label="Data Type" name="type-dropdown" />
            </ui:VisualElement>
            <ui:Label text="Data Input" />
            <ui:TextField name="data-text-field" multiline="true" vertical-scroller-visibility="Auto" />
            <ui:Button text="Import from File" name="import-file-button" />
            <ui:Button text="Process Data" name="process-data-button" class="action-button" />
        </ui:VisualElement>
        <ui:VisualElement name="column-mapping-container" display="none" class="step-container">
            <ui:Label text="Map Columns to Fields" class="step-title" />
            <ui:TextField name="data-preview-text-field" multiline="true" vertical-scroller-visibility="Auto" enabled="true" />
